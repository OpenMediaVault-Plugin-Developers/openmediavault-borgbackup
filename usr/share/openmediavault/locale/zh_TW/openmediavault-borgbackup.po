msgstr "將值當作歸檔名稱的前綴詞。"

msgid "Verify"
msgstr "驗證"

msgid "Weekly"
msgstr "每週"

msgid "Yearly"
msgstr "每年"

msgid "lz4 - super fast, low compression"
msgstr "lz4 - 超快、低壓縮"

msgid "lzma - even slower, even higher compression"
msgstr "lzma - 更慢、更高壓縮"

msgid "n/a"
msgstr "不適用"

msgid "repo ..."
msgstr "儲存庫 ..."

msgid "zlib - less fast, higher compression"
msgstr "zlib - 沒那麼快、高壓縮"

msgid "zstd - super fast, medium compression"
msgstr "zstd - 超快、中壓縮"
