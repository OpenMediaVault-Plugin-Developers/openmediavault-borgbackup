{
  "type": "config",
  "id": "conf.service.borgbackup.archive",
  "title": "BorgBackup archive",
  "queryinfo": {
    "xpath": "//services/borgbackup/archives/archive",
    "iterable": true,
    "idproperty": "uuid"
  },
  "properties": {
    "enable": {
      "type": "boolean",
      "default": true
    },
    "uuid": {
      "type": "string",
      "format": "uuidv4"
    },
    "name": {
      "type": "string"
    },
    "reporef": {
      "type": "string",
      "format": "uuidv4"
    },
    "compressiontype": {
      "type": "string",
      "enum": [
        "none",
        "zstd",
        "lz4",
        "lzma",
        "zlib"
      ],
      "default": "none"
    },
    "compressionratio": {
      "type": "integer",
      "minimum": 0,
      "maximum": 9,
      "default": 9
    },
    "onefs": {
      "type": "boolean",
      "default": false
    },
    "noatime": {
      "type": "boolean",
      "default": false
    },
    "include": {
      "type": "string"
    },
    "exclude": {
      "type": "string"
    },
    "hourly": {
      "type": "integer",
      "minimum": 0,
      "default": 0
    },
    "hourlyenable": {
      "type": "boolean",
      "default": false
    },
    "hourlymin": {
      "type": "integer",
      "minimum": 0,
      "maximum": 59,
      "default": 5
    },
    "daily": {
      "type": "integer",
      "minimum": 0,
      "default": 7
    },
    "dailyenable": {
      "type": "boolean",
      "default": true
    },
    "dailyhour": {
      "type": "integer",
      "minimum": 0,
      "maximum": 23,
      "default": 3
    },
    "dailymin": {
      "type": "integer",
      "minimum": 0,
      "maximum": 59,
      "default": 30
    },
    "weekly": {
      "type": "integer",
      "minimum": 0,
      "default": 4
    },
    "weeklyenable": {
      "type": "boolean",
      "default": false
    },
    "weeklyhour": {
      "type": "integer",
      "minimum": 0,
      "maximum": 23,
      "default": 3
    },
    "weeklymin": {
      "type": "integer",
      "minimum": 0,
      "maximum": 59,
      "default": 0
    },
    "monthly": {
      "type": "integer",
      "minimum": 0,
      "default": 3
    },
    "monthlyenable": {
      "type": "boolean",
      "default": false
    },
    "monthlyhour": {
      "type": "integer",
      "minimum": 0,
      "maximum": 23,
      "default": 2
    },
    "monthlymin": {
      "type": "integer",
      "minimum": 0,
      "maximum": 59,
      "default": 30
    },
    "yearly": {
      "type": "integer",
      "minimum": 0,
      "default": 0
    },
    "yearlyenable": {
      "type": "boolean",
      "default": false
    },
    "yearlyhour": {
      "type": "integer",
      "minimum": 0,
      "maximum": 23,
      "default": 2
    },
    "yearlymin": {
      "type": "integer",
      "minimum": 0,
      "maximum": 59,
      "default": 0
    },
    "compact": {
      "type": "boolean",
      "default": false
    },
    "cthreshold": {
      "type": "integer",
      "minimum": 0,
      "maximum": 99,
      "default": 10
    },
    "ratelimit": {
      "type": "integer",
      "minimum": 0,
      "default": 0
    },
    "list": {
      "type": "boolean",
      "default": true
    },
    "email": {
      "type": "boolean",
      "default": true
    },
    "prescript": {
      "type": "string"
    },
    "postscript": {
      "type": "string"
    },
    "basedir": {
      "type": "string"
    }
  }
}
